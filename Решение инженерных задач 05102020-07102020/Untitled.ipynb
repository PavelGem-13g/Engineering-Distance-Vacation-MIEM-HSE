   "source": [
    "with open('Text.txt', 'r') as f:\n",
    "    f = f.read()\n",
    "\n",
    "print(\"Bad marks = \",f.count(\"2\"))\n",
    "score_count = 0\n",
    "k = 0\n",
    "information = f.split(\" \")\n",
    "\n",
    "for i in information:\n",
    "    if i.isdigit():\n",
    "        score_count+=int(i)\n",
    "        k += 1\n",
    "\n",
    "print(\"Average score = \", score_count/k)"
   ]
